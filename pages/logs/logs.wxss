.log-list {
  display: flex;
  flex-direction: column;
  padding: 40rpx;
}
.log-item {
  margin: 10rpx;
}
.nav{
  display: flex;
  flex-direction: column;
  height: 30vh;
  width: 100vw;
  align-items: center;
  justify-content: center
}
.brand-img{
  height: 80%;
  width:100%
}

.avatar-wrapper {
  background: #1b82d1;
  padding: 25px 0;
}

.avatar-wrapper .avatar {
  margin: 0 auto;
  text-align: center;
}

.avatar-wrapper .avatar image {
  width: 100px;
  height: 100px;
}

.avatar-wrapper .text {
  text-align: center;
  color: #fff;
}

.weui-cell__hd image {
  margin-right: 5px;
  vertical-align: middle;
  width: 20px;
  height: 20px;
}

.textview{
   text-align: left; 
  line-height: 60rpx; 
  font-size: 15px;
  color: black
}